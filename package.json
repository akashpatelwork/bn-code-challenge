{
    "name": "bn-code-challenge",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "start": "npm run dev",
        "storybook": "start-storybook --port 3030",
        "storybook:fix": "npx npm-force-resolutions & npm install",
        "dev": "webpack-dev-server --mode development --hot --progress",
        "prettier": "prettier \"**/*.{ts,tsx,json}\" --check",
        "prettier:fix": "npm run prettier -- --write",
        "lint": "npm run lint:js && npm run lint:ts && npm run lint:css",
        "lint:ts": "tslint --project \".\" -t verbose",
        "lint:js": "eslint **/*.{ts,tsx}\"",
        "lint:css": "stylelint \"**/*.{ts,tsx}\""
    },
    "dependencies": {
        "@material-ui/core": "^4.11.0",
        "@material-ui/icons": "^4.9.1",
        "@material-ui/lab": "^4.0.0-alpha.56",
        "@material-ui/styles": "^4.10.0",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "styled-components": "^5.1.1",
        "ts-node": "^8.10.2"
    },
    "devDependencies": {
        "@babel/core": "^7.11.1",
        "@babel/preset-env": "^7.8.4",
        "@babel/preset-react": "^7.8.3",
        "@babel/preset-typescript": "^7.8.3",
        "@storybook/addon-a11y": "^5.3.19",
        "@storybook/addon-actions": "^5.3.19",
        "@storybook/addon-backgrounds": "^5.3.19",
        "@storybook/addon-console": "^1.2.1",
        "@storybook/addon-docs": "^5.3.19",
        "@storybook/addon-essentials": "^5.3.19",
        "@storybook/addon-knobs": "^5.3.19",
        "@storybook/addon-notes": "^5.3.19",
        "@storybook/addon-storysource": "^5.3.19",
        "@storybook/addon-viewport": "^5.3.19",
        "@storybook/addons": "^5.3.19",
        "@storybook/preset-typescript": "^3.0.0",
        "@storybook/react": "^5.3.19",
        "@types/react-dom": "^16.9.8",
        "@types/styled-components": "^5.1.2",
        "@typescript-eslint/eslint-plugin": "^3.8.0",
        "@typescript-eslint/parser": "^3.8.0",
        "babel-loader": "^8.0.6",
        "copy-webpack-plugin": "^5.0.3",
        "eslint": "^6.8.0",
        "eslint-config-clean-code": "^3.0.2",
        "eslint-plugin-import": "^2.20.2",
        "eslint-plugin-react": "^7.19.0",
        "eslint-plugin-react-hooks": "^3.0.0",
        "html-loader": "^1.1.0",
        "html-webpack-plugin": "^3.2.0",
        "husky": "^4.2.5",
        "lint-staged": "^10.2.11",
        "prettier": "^2.0.4",
        "react-docgen-typescript-loader": "^3.7.2",
        "react-is": "^16.13.1",
        "react-scripts": "^3.4.1",
        "resolve-url-loader": "^3.1.1",
        "stylelint": "^13.6.1",
        "stylelint-config-standard": "^20.0.0",
        "stylelint-config-styled-components": "^0.1.1",
        "stylelint-custom-processor-loader": "^0.6.0",
        "stylelint-processor-styled-components": "^1.10.0",
        "ts-loader": "^7.0.5",
        "tslint": "^5.17.0",
        "tslint-config-clean-code": "^4.0.3",
        "typescript": "^3.9.5",
        "webpack": "^4.34.0",
        "webpack-cli": "^3.3.4",
        "webpack-dev-server": "^3.11.0"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "engines": {
        "node": "12.x",
        "npm": "6.x"
    },
    "resolutions": {
        "@babel/preset-env": "^7.8.4",
        "styled-components": "^5"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.+{ts,tsx}": [
            "npm run prettier:fix",
            "npm run lint"
        ]
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/dannegm/bn-code-challenge.git"
    },
    "author": "Daniel Garc√≠a <dannegm@gmail.com>",
    "license": "MIT"
}
